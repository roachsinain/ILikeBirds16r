#驱动配置

menu "Drivers Configuration"

config PCF8574_DEFAULT_OUTPUT
    int "PCF8574 Default Output Value"
    default 0

config MATRIX_KEYS_ROWS
    int "Maximum number of rows of matrix keys"
    default 4
config MATRIX_KEYS_COLUMNS
    int "Maximum number of columns of matrix keys"
    default 4

config ESP12F_UART_RX_BUF_LEN
    int "Uart Rx Buffer Length"
    default 1024

config ESP12F_SOCKET_SEND_BUF_LEN
    int "Socket Send Buffer Length"
    default 1024

config ESP12F_SOCKET_RECV_BUF_LEN
    int "Socket Recv Buffer Length"
    default 1024

endmenu
